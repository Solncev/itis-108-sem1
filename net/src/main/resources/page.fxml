<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            fx:controller="com.solncev.fxml.controller.PageController"
            prefHeight="500" prefWidth="500">

    <children>
        <HBox id="Hbox" alignment="CENTER" spacing="10" AnchorPane.leftAnchor="10"
              AnchorPane.rightAnchor="10" AnchorPane.topAnchor="10">
            <children>
                <Label text="Search user by name:"/>
                <TextField fx:id="nickname" prefWidth="0" HBox.hgrow="ALWAYS"/>
                <Button fx:id="search" prefWidth="0" HBox.hgrow="ALWAYS"/>
            </children>
        </HBox>

        <VBox fx:id="users" AnchorPane.topAnchor="50" AnchorPane.rightAnchor="10"
              AnchorPane.leftAnchor="10">

        </VBox>
    </children>

</AnchorPane>